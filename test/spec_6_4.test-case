@@@ Example 6.4 Line Prefixes

@@@ source

plain: text
  lines
quoted: "text
  	lines"
block: |
  text
   	lines

@@@ events

{start_of_stream, {1, 1}}.
{start_of_document, {1, 1}}.
{start_of_mapping, {1, 1}, no_anchor, no_tag}.
{plain, {1, 1}, {1, 6}, no_anchor, no_tag
, [ {{1, 1}, {1, 6}, <<"plain">>} ]
}.
{plain, {1, 8}, {2, 8}, no_anchor, no_tag
, [ {{1, 8}, {1, 12}, <<"text">>}
  , {{1, 12}, {2, 3}, <<" ">>}
  , {{2, 3}, {2, 8}, <<"lines">>}
  ]
}.
{plain, {3, 1}, {3, 7}, no_anchor, no_tag
, [ {{3, 1}, {3, 7}, <<"quoted">>} ]
}.
{double, {3, 9}, {4, 10}, no_anchor, no_tag
, [ {{3, 10}, {3, 14}, <<"text">>}
  , {{3, 14}, {4, 4}, <<" ">>}
  , {{4, 4}, {4, 9}, <<"lines">>}
  ]
}.
{plain, {5, 1}, {5, 6}, no_anchor, no_tag
, [ {{5, 1}, {5, 6}, <<"block">>} ]
}.
{literal, {5, 8}, {8, 1}, no_anchor, no_tag
, [ {{6, 3}, {6, 7}, <<"text">>}
  , {{6, 7}, {7, 3}, <<"\n">>}
  , {{7, 3}, {7, 10}, <<" \tlines">>}
  , {{7, 10}, {8, 1}, <<"\n">>}
  ]
}.
{end_of_mapping, {8, 1}}.
{end_of_document, {8, 1}}.
{end_of_stream, {8, 1}}.
